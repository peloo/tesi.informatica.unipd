{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:BatchGetItem",
                "dynamodb:GetItem",
                "dynamodb:Query",
                "dynamodb:Scan",
                "dynamodb:BatchWriteItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem"
            ], "Resource": "*"
        },{
            "Effect": "Allow",
            "Action": [ "logs:CreateLogStream", "logs:PutLogEvents"], 
            "Resource": "arn:aws:logs:eu-west-1:679871574158:*"
        },{
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "*"
        },{
            "Effect": "Allow",
            "Action": [ "ses:*" ], 
            "Resource": "*"
        }
    ]
}